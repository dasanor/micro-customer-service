{
  "services": {
    "name": "customer",
    "version": "v1",
    "style": "RPC"
  },
  "schemas": {
    "createCustomer": "./schemas/createCustomerSchema",
    "updateCustomer": "./schemas/updateCustomerSchema",
    "infoCustomer": "./schemas/infoCustomerSchema",
    "removeCustomer": "./schemas/removeCustomerSchema",
    "checkCredentials": "./schemas/checkCredentialsSchema",
    "createAddress": "./schemas/createAddressSchema",
    "updateAddress": "./schemas/updateAddressSchema",
    "infoAddress": "./schemas/infoAddressSchema",
    "removeAddress": "./schemas/removeAddressSchema"
  },
  "models": {
    "Customer": "./models/customerModel"
  },
  "bus": {
    "engines": {
      "local": {
        "handler": "./local"
      },
      "amqp": {
        "handler": "./amqp",
        "url": "amqp://bus"
      }
    },
    "channels": {
      "customers": {
        "name": "CUSTOMERS",
        "engine": "amqp"
      }
    }
  },
  "aggregateItems": true,
  "bcryptSalt": 10
}
